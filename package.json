{
  "name": "nextjs-shadcn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 --turbopack",
    "dev:fast": "NEXT_PRIVATE_SKIP_SIZE_LIMIT=true next dev -H 0.0.0.0 --turbopack",
    "dev:profile": "next dev -H 0.0.0.0 --experimental-debug",
    "build": "next build --no-lint",
    "build:analyze": "ANALYZE=true next build --no-lint",
    "start": "next start",
    "workers": "node scripts/start-workers.js",
    "workers:dev": "NODE_ENV=development node scripts/start-workers.js",
    "lint": "bunx tsc --noEmit && next lint",
    "lint:fix": "next lint --fix",
    "format": "bunx biome format --write",
    "cors:setup": "node scripts/setup-cors.js",
    "cors:test": "node scripts/test-cors.js",
    "cors:test:prod": "node scripts/test-cors.js https://mailgenius.com",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "queue:test": "jest src/lib/queue/__tests__",
    "db:migrate": "psql -f database/migrations/009_queue_system.sql",
    "performance:test": "tsx src/tests/performance/scripts/run-tests.ts run",
    "performance:test:all": "tsx src/tests/performance/scripts/run-tests.ts run --suite all --monitor --report",
    "performance:test:load": "tsx src/tests/performance/scripts/run-tests.ts run --suite load --monitor --report",
    "performance:test:benchmark": "tsx src/tests/performance/scripts/run-tests.ts run --suite benchmark --monitor --report",
    "performance:test:critical": "tsx src/tests/performance/scripts/run-tests.ts run --suite critical --monitor --report",
    "performance:test:quick": "tsx src/tests/performance/scripts/run-tests.ts run --suite quick --monitor --report",
    "performance:test:2mm": "tsx src/tests/performance/scripts/load-test-2mm.ts run --monitor --report",
    "performance:test:2mm:import": "tsx src/tests/performance/scripts/load-test-2mm.ts run --scenario import --monitor --report",
    "performance:test:2mm:email": "tsx src/tests/performance/scripts/load-test-2mm.ts run --scenario email --monitor --report",
    "performance:test:2mm:database": "tsx src/tests/performance/scripts/load-test-2mm.ts run --scenario database --monitor --report",
    "performance:list": "tsx src/tests/performance/scripts/run-tests.ts list",
    "performance:validate": "tsx src/tests/performance/scripts/run-tests.ts validate",
    "performance:monitor": "tsx src/tests/performance/monitoring/performance-monitor.ts",
    "performance:report": "tsx src/tests/performance/reports/report-generator.ts",
    "ci:test": "npm run test && npm run lint",
    "ci:performance": "npm run performance:test:critical",
    "ci:performance:full": "npm run performance:test:all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.50.5",
    "@tanstack/react-query": "^5.83.0",
    "@types/dompurify": "^3.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "lucide-react": "^0.525.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.3.2",
    "next-themes": "^0.4.6",
    "openai": "^5.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.60.0",
    "recharts": "^3.1.0",
    "resend": "^4.6.0",
    "same-runtime": "^0.0.1",
    "sonner": "^2.0.6",
    "stream-transform": "^3.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.5",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "commander": "^12.0.0",
    "tsx": "^4.19.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/eslintrc": "^3.3.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.50",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "@types/bull": "^4.10.0"
  }
}
